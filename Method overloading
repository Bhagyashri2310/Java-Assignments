public class Multiplier {

    
    public int multiply(int a, int b) {
        return a * b;
    }

    
    public int multiply(int a, int b, int c) {
        return a * b * c;
    }

   
    public double multiply(double a, double b) {
        return a * b;
    }

    public static void main(String[] args) {
        Multiplier m = new Multiplier();

        System.out.println("Multiplication of 2 integers: " + m.multiply(4, 5));
        System.out.println("Multiplication of 3 integers: " + m.multiply(2, 3, 4));
        System.out.println("Multiplication of 2 doubles: " + m.multiply(2.5, 4.0));
    }
}